{
  "name": "mikazuki-munechika",
  "version": "1.0.0",
  "description": "Jekyll site for GitHub Pages",
  "scripts": {
    "lint": "npm run lint:markdown && npm run lint:yaml",
    "lint:markdown": "markdownlint-cli2",
    "lint:yaml": "prettier --check \"**/*.yml\" \"**/*.yaml\"",
    "lint:fix": "npm run lint:fix:markdown && npm run lint:fix:yaml",
    "lint:fix:markdown": "markdownlint-cli2 --fix",
    "lint:fix:yaml": "prettier --write \"**/*.yml\" \"**/*.yaml\""
  },
  "devDependencies": {
    "markdownlint-cli2": "^0.20.0",
    "prettier": "^3.3.3"
  }
}
